<h1>Listing albums</h1>

<table>
  <tr>
  </tr>

<% for album in @albums %>
  <tr>
    <td class="name"><%= album.name %></td>
    <td class="show"><%= link_to 'Show', admin_album_path(album) %></td>
    <td class="delete"><%= link_to 'Destroy', [:admin, album], data: {confirm: 'Are you sure?'}, method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New album', new_admin_album_path %>
